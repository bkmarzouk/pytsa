# Ignore compiled python files everywhere
**/*.pyc
**/__pycache__
**/*.egg
**/*.egg-info
**/*.so
**/build
**/dist
/pytsa/pyt/lib/python3.8/site-packages/*

# Ignore .idea
.idea

# Ignore builds dir
samplers/

# Ignore Installation Directory (except for examples)
/example_models/*
!/example_models/dquad_euclidean.py
!/example_models/dquad_2sphere.py
!/example_models/quad_euclidean.py

/pytsa/models/*
!/pytsa/models/__init__.py

# Ignore sampler builds, but keep directory
/pytsa/sampler/builds/*
!/pytsa/sampler/builds/.gitkeep

# Ignore sampler configs (except for examples)
/pytsa/sampler/configs/*
!/pytsa/sampler/configs/EXAMPLE.py
!/pytsa/sampler/configs/EXAMPLE_NC.py
!/pytsa/sampler/configs/simple_example.py
!/pytsa/sampler/setup_sampler.py

# Ignore cached symbolic files
/pytsa/sym_cache/*

# Ignore .h header files that are generated from proto
/pytsa/cppt/fieldmetric.h
/pytsa/cppt/potential.h

# Keep .gitignore
!.gitignore
